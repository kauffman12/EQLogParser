<UserControl x:Class="EQLogParser.LineChart"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
  <lvc:CartesianChart x:Name="lvcChart" Zoom="Xy" Pan="Xy" LegendLocation="Right" DisableAnimations="True" MouseDoubleClick="Chart_DoubleClick">
    <lvc:CartesianChart.AxisY>
      <lvc:Axis Title="Data Per Second" MinValue="0"></lvc:Axis>
    </lvc:CartesianChart.AxisY>
    <lvc:CartesianChart.AxisX>
      <lvc:Axis></lvc:Axis>
    </lvc:CartesianChart.AxisX>
    <lvc:CartesianChart.Series>
      <lvc:LineSeries></lvc:LineSeries>
    </lvc:CartesianChart.Series>
  </lvc:CartesianChart>
</UserControl>
