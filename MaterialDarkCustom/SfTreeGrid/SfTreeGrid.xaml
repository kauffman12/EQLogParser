                                                   Converter={StaticResource filteredFromCheckVisibilityConverter},
                                                   ConverterParameter=reverse}">
                                    <Path.Margin>
                                        <Thickness>4,5,4,4</Thickness>
                                    </Path.Margin>
                                    <Path.Data>
                                        <PathGeometry>M14.573975,0 L16,1.401977 4.848999,12.742 0,7.8109712 1.4259644,6.4089942 4.848999,9.8900108 z</PathGeometry>
                                    </Path.Data>
                                    <Path.RenderTransform>
                                        <TransformGroup>
                                            <TransformGroup.Children>
                                                <RotateTransform Angle="0" />
