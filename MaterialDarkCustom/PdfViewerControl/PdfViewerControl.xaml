                                    <ScrollViewer
                                        x:Name="Thumb_ScrollViewer"
                                        HorizontalScrollBarVisibility="Auto"
                                        VerticalScrollBarVisibility="Auto">
                                        <Border
                                            Background="{StaticResource ContentBackground}"
                                            BorderBrush="{StaticResource BorderAlt}">
                                            <Grid Name="Thumb_Grid" />
                                        </Border>
                                    </ScrollViewer>
                                </Grid>
                                <Border
                                    Grid.Row="2"
                                    Background="{StaticResource ContentBackground}"
                                    BorderBrush="{StaticResource BorderAlt}"
