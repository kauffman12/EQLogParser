                                <Button
                                    Name="PART_Close"
                                    Grid.Row="0"
                                    Width="24"
                                    Height="24"
                                    Margin="4px"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    IsEnabled="True"
                                    Style="{StaticResource WPFGlyphButtonStyle}">
