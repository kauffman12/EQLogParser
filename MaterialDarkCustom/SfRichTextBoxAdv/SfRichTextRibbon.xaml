                                                                <Setter.Value>
                                                                    <ControlTemplate TargetType="Button">
                                                                        <Grid>
                                                                            <Border x:Name="PART_Border" Background="Gray" Opacity="0.1">
                                                                                <Border.BitmapEffect>
                                                                                    <BlurBitmapEffect />
                                                                                </Border.BitmapEffect>
                                                                            </Border>
                                                                            <ContentPresenter Margin="3"/>
                                                                        </Grid>
