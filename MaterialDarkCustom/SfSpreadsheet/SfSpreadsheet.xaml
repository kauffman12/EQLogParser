                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="30" MinWidth="30"/>
                                    <ColumnDefinition Width="30" MinWidth="30"/>
                                    <ColumnDefinition Width="0" MinWidth="0"/>
                                </Grid.ColumnDefinitions>
                                <shared:ButtonAdv x:Name="CancelEditButton" Grid.Column="0"
