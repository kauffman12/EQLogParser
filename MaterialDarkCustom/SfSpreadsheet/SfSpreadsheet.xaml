                                <DataGridTextColumn x:Name="Formula" Width="*"  Header="{spreadsheet:SpreadsheetLocalizationResourceExtension ResourceName=Formula}" Binding="{Binding Path=Formula}"></DataGridTextColumn>
                            </DataGrid.Columns>
