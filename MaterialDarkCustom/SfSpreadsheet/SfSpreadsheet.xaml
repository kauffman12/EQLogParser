                        <TextBlock x:Name="CellsFoundDetail" FontSize="12" Grid.Row="3" Margin="9,6,0,3" Visibility="Collapsed"></TextBlock>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

